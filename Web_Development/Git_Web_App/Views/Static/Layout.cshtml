<!DOCTYPE html>
<html lang="en">
<head>
    <title>Test.cshtml</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="@Url.Content("~/bootstrap/3.3.7/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
</head>

<body>
    <nav class="navbar navbar-default">
        <div class="container-fluid">
            <div class="navbar-header">
                <a class="navbar-brand" href="#">WebSiteName</a>
            </div>
            <ul class="nav navbar-nav">
                <li class="active">@Html.RouteLink("Home", new { action = "Index", controller = "Home", area = "Home" })</li>
                <li>@Html.RouteLink("Setup", new { action = "Index", controller = "Url", area = "Url" })</li>
            </ul>
        </div>
    </nav>
    
    <div class="container"> @RenderBody() </div>

    <div class="footer">
        <div class="container text-center">
            © 2015 Copyright
        </div>
    </div>
    
</body>
</html>
